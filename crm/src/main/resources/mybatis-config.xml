<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!--核心配置文件-->
<configuration>
    <!--    映射mybatis配置文件-->
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <typeAliases>
        <package name="com.haoyang.settings.pojo"/>
        <package name="com.haoyang.workbench.pojo"/>
    </typeAliases>
<!--    指向动态代理的mapper，自动生成动态代理mapper类-->
    <mappers>
        <package name="com.haoyang.settings.mapper"/>
        <package name="com.haoyang.workbench.mapper"/>
    </mappers>
</configuration>